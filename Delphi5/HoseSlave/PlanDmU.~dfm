  object uRendelesProgram: TUpdateSQL
    ModifySQL.Strings = (
      'update RendelesProgram'
      'set'
      '  Rendelésszám = :Rendelésszám,'
      '  Pozíció = :Pozíció,'
      '  Revízió = :Revízió,'
      '  Megrendelõ = :Megrendelõ,'
      '  Felhasználó = :Felhasználó,'
      '  Gyártási_területkód = :Gyártási_területkód,'
      '  Specifikáció = :Specifikáció,'
      '  Cikkszám = :Cikkszám,'
      '  db = :db,'
      '  Hossz = :Hossz,'
      '  Lélekanyagkód = :Lélekanyagkód,'
      '  Sodronytipuskód = :Sodronytipuskód,'
      '  SodrMennyiség = :SodrMennyiség,'
      '  Normaidõ = :Normaidõ,'
      '  Státusz = :Státusz,'
      '  Határidõ = :Határidõ,'
      '  Rögzítette = :Rögzítette,'
      '  RögzIdõpont = :RögzIdõpont,'
      '  ProgÉv = :ProgÉv,'
      '  ProgHónap = :ProgHónap,'
      '  Megjegyzés = :Megjegyzés,'
      '  Kiadott_átvételi = :Kiadott_átvételi,'
      '  Érték = :Érték,'
      '  Lángálló = :Lángálló,'
      '  Belsõgégecsõ = :Belsõgégecsõ,'
      '  Külsõgégecsõ = :Külsõgégecsõ,'
      '  Interlock = :Interlock,'
      '  Poliamid = :Poliamid,'
      '  Külsõvédelem = :Külsõvédelem,'
      '  Inspektor = :Inspektor,'
      '  Festés = :Festés,'
      '  Sorrend = :Sorrend'
      'where'
      '  Id = :OLD_Id')
    InsertSQL.Strings = (
      'insert into RendelesProgram'
      
        '  (Rendelésszám, Pozíció, Revízió, Megrendelõ, Felhasználó, Gyár' +
        'tási_területkód, '
      
        '   Specifikáció, Cikkszám, db, Hossz, Lélekanyagkód, Sodronytipu' +
        'skód, SodrMennyiség, '
      
        '   Normaidõ, Státusz, Határidõ, Rögzítette, RögzIdõpont, ProgÉv,' +
        ' ProgHónap, '
      
        '   Megjegyzés, Kiadott_átvételi, Érték, Lángálló, Belsõgégecsõ, ' +
        'Külsõgégecsõ, '
      
        '   Interlock, Poliamid, Külsõvédelem, Inspektor, Festés, Sorrend' +
        ')'
      'values'
      
        '  (:Rendelésszám, :Pozíció, :Revízió, :Megrendelõ, :Felhasználó,' +
        ' :Gyártási_területkód, '
      
        '   :Specifikáció, :Cikkszám, :db, :Hossz, :Lélekanyagkód, :Sodro' +
        'nytipuskód, '
      
        '   :SodrMennyiség, :Normaidõ, :Státusz, :Határidõ, :Rögzítette, ' +
        ':RögzIdõpont, '
      
        '   :ProgÉv, :ProgHónap, :Megjegyzés, :Kiadott_átvételi, :Érték, ' +
        ':Lángálló, '
      
        '   :Belsõgégecsõ, :Külsõgégecsõ, :Interlock, :Poliamid, :Külsõvé' +
        'delem, '
      '   :Inspektor, :Festés, :Sorrend)')
    DeleteSQL.Strings = (
      'delete from RendelesProgram'
      'where'
      '  Id = :OLD_Id')
    Left = 176
    Top = 96
  end
